    "s = input()\n",
    "list=s.split(\",\")\n",
    "Count1=0\n",
    "Count2=0\n",
    "for i in list:\n",
    "    if(int(i)%2==0):\n",
    "        Count1 += 1\n",
    "    else:\n",
    "        Count2 += 1\n",
    "print(\"Number of even numbers : \",Count1)\n",
    "print(\"Number of even numbers : \",Count2)"
